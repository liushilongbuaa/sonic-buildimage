
SPATH       := $($(VNET_MONITOR_PY3)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/vnet-monitor.mk rules/vnet-monitor.dep   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(VNET_MONITOR_PY3)_CACHE_MODE  := GIT_CONTENT_SHA 
$(VNET_MONITOR_PY3)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(VNET_MONITOR_PY3)_DEP_FILES   := $(DEP_FILES)
$(VNET_MONITOR_PY3)_SMDEP_FILES := $(SMDEP_FILES)
$(VNET_MONITOR_PY3)_SMDEP_PATHS := $(SPATH)

