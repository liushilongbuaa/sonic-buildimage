!
! template: bgpd/templates/msft.general/v6.mgmttor/policy.conf.j2
!
!
bgp as-path access-list LOCALROUTES permit ^$
!
ipv6 prefix-list DEFAULT_IPV6 permit ::/0
!
route-map TO_VPN_OOB_V6 deny 10
  match ipv6 address prefix-list DEFAULT_IPV6
!
route-map TO_VPN_OOB_V6 permit 20
  match as-path LOCALROUTES
  set community 8075:316 8075:9120 additive
!
route-map TO_VPN_OOB_V6 permit 1000
  set community 8075:9120 additive
!
route-map FROM_VPN_OOB_V6 permit 10
 set ipv6 next-hop prefer-global
 on-match next
!
route-map FROM_VPN_OOB_V6 permit 20
  match ipv6 address prefix-list DEFAULT_IPV6
  set as-path prepend 65532
!
route-map FROM_VPN_OOB_V6 deny 1000
!
! end of template: bgpd/templates/msft.general/v6.mgmttor/policy.conf.j2
!
