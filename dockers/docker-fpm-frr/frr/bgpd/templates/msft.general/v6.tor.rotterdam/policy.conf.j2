!
! template: bgpd/templates/msft.general/v6.tor.rotterdam/policy.conf.j2
!
ip community-list standard COMM_SDN_APPLIANCE permit 8075:9302
!
route-map FROM_TIER1_V6 permit 1
  set ipv6 next-hop prefer-global
!
route-map TO_TIER1_V6 permit 100
  match ipv6 address prefix-list LOCAL_VLAN_IPV6_PREFIX
  set community 8075:9302
!
route-map TO_TIER1_V6 permit 200
  match ipv6 address prefix-list PL_LoopbackV6
  set community 8075:9302
!
route-map TO_TIER1_V6 permit 300
  match community COMM_SDN_APPLIANCE
!
route-map TO_TIER1_V6 deny 400
!
! end of template: bgpd/templates/msft.general/v6.tor.rotterdam/policy.conf.j2
!
