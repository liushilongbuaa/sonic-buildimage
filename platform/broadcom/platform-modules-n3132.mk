# Cisco Nexus 3132 Platform modules

CISCO_N3132_PLATFORM_MODULE_VERSION = 1.0

export CISCO_N3132_PLATFORM_MODULE_VERSION

CISCO_N3132_PLATFORM_MODULE = platform-modules-n3132_$(CISCO_N3132_PLATFORM_MODULE_VERSION)_amd64.deb
$(CISCO_N3132_PLATFORM_MODULE)_SRC_PATH = $(PLATFORM_PATH)/Networking-acs-platform-modules-n3132
$(CISCO_N3132_PLATFORM_MODULE)_DEPENDS += $(LINUX_HEADERS) $(LINUX_HEADERS_COMMON)
$(CISCO_N3132_PLATFORM_MODULE)_PLATFORM = x86_64-m3000-r0 x86_64-m3000-r1
SONIC_DPKG_DEBS += $(CISCO_N3132_PLATFORM_MODULE)
